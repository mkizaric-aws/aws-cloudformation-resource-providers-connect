{
    "typeName": "AWS::Connect::ContactFlowVersion",
    "description": "Resource Type Definition for ContactFlowVersion",
    "sourceUrl": "https://github.com/aws-cloudformation/aws-cloudformation-resource-providers-connect",
    "definitions": {},
    "properties": {
        "ContactFlowVersionARN": {
            "description": "The identifier of the contact flow version (ARN).",
            "type": "string",
            "pattern": "^arn:aws[-a-z0-9]*:connect:[-a-z0-9]+:[0-9]{12}:instance/[-a-zA-Z0-9]+/contact-flow/[-a-zA-Z0-9]+/version/[0-9]+$",
            "minLength": 1,
            "maxLength": 500
        },
        "ContactFlowARN": {
            "description": "The identifier of the contact flow (ARN) with a version suffix that identifies this version.",
            "type": "string",
            "pattern": "^arn:aws[-a-z0-9]*:connect:[-a-z0-9]+:[0-9]{12}:instance/[-a-zA-Z0-9]+/contact-flow/[-a-zA-Z0-9]+:[0-9]+$",
            "minLength": 1,
            "maxLength": 500
        },
        "ContactFlowId": {
            "description": "The identifier of the contact flow (ARN|UUID) this version is tied to.",
            "type": "string",
            "pattern": "^arn:aws[-a-z0-9]*:connect:[-a-z0-9]+:[0-9]{12}:instance/[-a-zA-Z0-9]+/contact-flow/[-a-zA-Z0-9]+$|^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
            "minLength": 1,
            "maxLength": 500
        },
        "Version": {
            "description": "The version number of this revision",
            "type": "integer"
        },
        "Description": {
            "description": "The description of the version.",
            "type": "string",
            "maxLength": 500
        }
    },
    "additionalProperties": false,
    "required": [
        "ContactFlowVersionARN",
        "ContactFlowARN",
        "ContactFlowId",
        "Version"
    ],
    "primaryIdentifier": [
        "/properties/ContactFlowVersionARN"
    ],
    "additionalIdentifiers": [
        ["/properties/ContactFlowARN"]
    ],
    "readOnlyProperties": [
        "/properties/ContactFlowVersionARN",
        "/properties/ContactFlowARN",
        "/properties/Version"
    ],
    "createOnlyProperties": [
        "/properties/ContactFlowId",
        "/properties/Description"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "initech:CreateReport"
            ]
        },
        "read": {
            "permissions": [
                "initech:DescribeReport"
            ]
        },
        "update": {
            "permissions": [
                "initech:UpdateReport"
            ]
        },
        "delete": {
            "permissions": [
                "initech:DeleteReport"
            ]
        },
        "list": {
            "permissions": [
                "initech:ListReports"
            ]
        }
    }
}
